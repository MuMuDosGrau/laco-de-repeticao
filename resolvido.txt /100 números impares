//fazer contador de 1 a 100 que mostre apenas os numeros impares.

var qtNumeros = 100
var contador = 0
var acumulador = 0

while (contador <= qtNumeros) {
  acumulador = acumulador + contador

  if((contador %2)===1){
    acumulador = acumulador + contador
  }

  console.log(contador, acumulador)
  contador++

}
